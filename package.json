{
  "name": "agda-mode",
  "displayName": "agda-mode",
  "description": "",
  "version": "0.0.1",
  "type": "module",
  "engines": {
    "vscode": "^1.106.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "agda-mode.openPanel",
        "title": "Open the Webview panel"
      },
      {
        "command": "agda-mode.sendMessage",
        "title": "Send a message to the Webview panel"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "build:webview": "cd modules/agda-mode && agda --js --js-es6 --compile-dir=../../out --library-file=../../libraries --no-default-libraries src/Webview.agda",
    "build:extension": "cd modules/agda-mode && agda --js --js-es6 --compile-dir=../../out --library-file=../../libraries --no-default-libraries src/Extension.agda",
    "build": "npm run build:webview && npm run build:extension && cp modules/agda-mode/src/extension.js out"
  }
}
